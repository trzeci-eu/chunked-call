{
  "name": "chunked-call",
  "version": "0.0.4",
  "description": "Split blocking execution to small chunks.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trzecieu/chunked-call.git"
  },
  "keywords": [
    "typescript",
    "chunks",
    "chunked",
    "async",
    "execution"
  ],
  "author": "Piotr Paczkowski <kontakt@trzeci.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trzecieu/chunked-call/issues"
  },
  "homepage": "https://github.com/trzecieu/chunked-call#readme",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  }
}
